{
  "devDependencies": {
    "jest": "^29.7.0",
    "prisma": "^6.15.0",
    "supertest": "^6.3.3"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "postgres": "^3.4.7",
    "tsx": "^4.20.5"
  },
  "scripts": {
    "start": "node index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:db:start": "docker-compose -f docker-compose.test.yml up -d",
    "test:db:stop": "docker-compose -f docker-compose.test.yml down",
    "test:db:clean": "docker-compose -f docker-compose.test.yml down -v",
    "seed": "node prisma/seed.js",
    "migrate-dev": "npx prisma migrate dev && npx tsx prisma/triggers.ts"
  }
}
